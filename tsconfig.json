{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
      "paths": {
      "src/*": ["./src/*"],
      "utilities":["./src/utilities"],
      "utilities/*": ["./src/utilities/*"],
      "components": ["./src/components"],
      "components/*": ["./src/components/*"],
      "screens": ["./src/screens"],
      "screens/*": ["./src/screens/*"],
      "hooks": ["./src/hooks"],
      "hooks/*": ["./src/hooks/*"],
      "store": ["./src/store"],
      "store/*": ["./src/store/*"],
      }
  },
  "include": ["axios.d.ts", "src/**/*.tsx","src/**/*.ts"],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}